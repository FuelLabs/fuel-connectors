{
  "$schema": "https://unpkg.com/@changesets/config@3.0.0/schema.json",
  "privatePackages": {
    "version": true,
    "tag": false
  },
  "commit": false,
  "fixed": [
    [
      "@fuels/connectors",
      "@fuel-connectors/*",
      "@fuels/react",
      "@e2e-tests/runner"
    ]
  ],
  "linked": [],
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
  "ignore": ["react-app", "react-next", "@e2e-tests/react-next"],
  "snapshot": {
    "useCalculatedVersion": true,
    "prereleaseTemplate": "{tag}-{commit}"
  }
}
