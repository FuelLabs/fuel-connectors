{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "ES2022", "ESNext.Array"],
    "module": "es2022",
    "moduleResolution": "Node",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "declaration": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@fuel-packages/*": ["../*"],
    }
  },
  "exclude": ["./dist/**/*"]
}
